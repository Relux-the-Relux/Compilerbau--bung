Grammar

    0 $accept: start $end

    1 start: program

    2 program: %empty
    3        | program declassignment ';'
    4        | program functiondefinition

    5 $@1: %empty

    6 functiondefinition: type ID $@1 '(' opt_parameterlist ')' '{' statementlist '}'

    7 opt_parameterlist: %empty
    8                  | parameterlist

    9 parameterlist: parameter
   10              | parameter ',' parameterlist

   11 parameter: type ID

   12 functioncall: ID '(' opt_argumentlist ')'

   13 opt_argumentlist: %empty
   14                 | argumentlist

   15 argumentlist: assignment
   16             | argumentlist ',' assignment

   17 statementlist: %empty
   18              | statementlist statement

   19 $@2: %empty

   20 block: '{' $@2 statementlist '}'

   21 statement: ifstatement
   22          | forstatement
   23          | whilestatement
   24          | returnstatement ';'
   25          | dowhilestatement ';'
   26          | printf ';'
   27          | declassignment ';'
   28          | statassignment ';'
   29          | functioncall ';'
   30          | block

   31 $@3: %empty

   32 $@4: %empty

   33 ifstatement: KW_IF '(' assignment ')' $@3 statement $@4 opt_else

   34 opt_else: %empty

   35 $@5: %empty

   36 opt_else: KW_ELSE $@5 statement

   37 $@6: %empty

   38 forstatement: KW_FOR '(' $@6 declassignment ';' expr ';' statassignment ')' statement

   39 $@7: %empty

   40 forstatement: KW_FOR '(' $@7 statassignment ';' expr ';' statassignment ')' statement

   41 $@8: %empty

   42 dowhilestatement: KW_DO $@8 statement KW_WHILE '(' assignment ')'

   43 $@9: %empty

   44 whilestatement: KW_WHILE '(' assignment ')' $@9 statement

   45 returnstatement: KW_RETURN
   46                | KW_RETURN assignment

   47 printf: KW_PRINTF '(' assignment ')'
   48       | KW_PRINTF '(' CONST_STRING ')'

   49 declassignment: type ID
   50               | type ID '=' assignment

   51 type: KW_BOOLEAN
   52     | KW_FLOAT
   53     | KW_INT
   54     | KW_VOID

   55 statassignment: ID '=' assignment

   56 assignment: ID '=' assignment
   57           | expr

   58 expr: simpexpr
   59     | simpexpr EQ simpexpr
   60     | simpexpr NEQ simpexpr
   61     | simpexpr LEQ simpexpr
   62     | simpexpr GEQ simpexpr
   63     | simpexpr LSS simpexpr
   64     | simpexpr GRT simpexpr

   65 simpexpr: simpexpr '+' simpexpr
   66         | simpexpr '-' simpexpr
   67         | simpexpr OR simpexpr
   68         | simpexpr '*' simpexpr
   69         | simpexpr '/' simpexpr
   70         | simpexpr AND simpexpr
   71         | '-' simpexpr
   72         | CONST_INT
   73         | CONST_FLOAT
   74         | CONST_BOOLEAN
   75         | functioncall
   76         | ID
   77         | '(' assignment ')'


