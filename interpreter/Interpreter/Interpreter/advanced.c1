int fibonacci(int n)
{
	if (n <= 1)
		return n;
	
	return fibonacci(n - 1) + fibonacci(n - 2);
}

void main()
{
	for (int n = 1; n < 30; n = n + 1)
		printf(fibonacci(n));
}

